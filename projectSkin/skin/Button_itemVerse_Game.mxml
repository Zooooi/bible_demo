<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" xmlns:style="projectComponents.bible.viewers.style.*" xmlns:filters="projectSkin.filters.*" cacheAsBitmap="true" >
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="down" />
		<s:State name="over" />
		<s:State name="up" />
	</s:states>

	<!-- SkinParts
	name=iconDisplay, type=spark.primitives.BitmapImage, required=false
	name=labelDisplay, type=spark.core.IDisplayText, required=false
	-->
	<s:BitmapImage source="@Embed('assets/imgs/bible/button/game1.svg')" width="100%" height="100%" smooth="true" alpha.disabled="0.8" />
	<s:BitmapImage source.down="@Embed('assets/imgs/bible/button/game1_ov.svg')" width="100%" height="100%" smooth="true" alpha.disabled="0.8" />
</s:Skin>
